<!--
  Voting Commands

  Commands related to the voting system
-->

  <command name="vote">
    <permission level="1"/>
    <syntax value="!vote kick &lt;username&gt; &lt;reason&gt; OR !vote map &lt;mapname&gt; OR !vote cyclemap OR !vote suddendeath OR !vote powerup OR !vote powerdown"/>
    <help value="Kick and Map Voting. Username can be a partial username. Mapname can be a partial mapname. Cyclemap will switch to the next map in the rotation."/>
    <enabled value="1"/>
